// This file is managed by Ansible. Do not edit it manually.

logging {
    channel update_debug {
        file "/var/log/bind/update.log" versions 3 size 100k;
        severity        debug;
        print-severity  yes;
        print-time      yes;
    };

    channel security_info {
        file "/var/log/bind/security.log" versions 3 size 100k;
        severity        info;
        print-severity  yes;
        print-time      yes;
    };

    channel bind_log {
        file "/var/log/bind/bind.log" versions 3 size 1m;
        severity        info;
        print-category  yes;
        print-severity  yes;
        print-time      yes;
    };

    category default {
        bind_log;
    };
    category lame-servers {
        null;
    };
    category update {
        update_debug;
    };
    category update-security {
        update_debug;
    };
    category security {
        security_info;
    };

};

